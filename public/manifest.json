{
   "manifest_version": 3,
   "name": "独自用語集検索ツール",
   "version": "1.0.0",
   "permissions": [
      "storage"
   ],
   "background": {
      "service_worker": "/assets/background.js"
   },
   "content_scripts": [
      {
         "js": ["/assets/content.js"],
         "matches": ["<all_urls>"]
      }
   ],
   "action": {
      "default_popup": "/popup/index.html"
   }
}
